<p style="color: green"><%= notice %></p>

<div id="maitre-widget"></div>

<h1>Users</h1>

<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user %>
    </p>
    <hr></hr>
  <% end %>
</div>

<%= link_to "New user", new_user_path %>
 | 
<%= button_to "Log out", destroy_user_session_path, method: :delete %>

<br>

<%= paginate @users %>
